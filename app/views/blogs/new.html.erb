<h2>Create A Blog</h2>

<%= form_for @blogs do |f| %>
  <%= f.label :user_name, class: "form_label" %>
  <%= f.text_field :user_name %>

  <%= f.label :title, class: "form_label" %>
  <%= f.text_field :title %>

  <%= f.label :date_traveled, class: "form_label" %>
  <%= f.text_field :date_traveled %>

  <%= f.label :country, class: "form_label" %>
  <%= f.text_field :country %>

  <%= f.label :city, class: "form_label" %>
  <%= f.text_field :city %>

  <%= f.label :place, class: "form_label"%>
  <%= f.text_field :place %>

  <%= f.label :article, class: "form_label" %>
  <%= f.text_field :article %>

  <%= f.label :photo_url, class: "form_label" %>
  <%= f.text_field :photo_url %>

  <%= f.submit "Create", class: "form_label" %>
<% end %>
