<h1>Blog#index</h1>
<p>Find me in app/views/blog/index.html.erb</p>

<ul>
  <% @blogs.each do |blog| %>
    <li><a href="/blogs/<%= blog.id %>">
        <%= blog.user_name %></a></li>
    <li><%= blog.title %></li>
    <li><%= blog.date_traveled %></li>
    <li><%= blog.article %></li>

  <% end %>
</ul>
